Private Sub Timer1_Timer()
On Error Resume Next

    Text1(0) = Time()
    Text1(1) = Format(Date, "dddd, mmm d yyyy")

End Sub

Private Sub Timer2_Timer()
On Error Resume Next

    Me.Show
    Unload Form2
    
    List1.ListIndex = 0
    List2.ListIndex = 0
    ListView1.SetFocus

    Timer2.Enabled = False
End Sub

